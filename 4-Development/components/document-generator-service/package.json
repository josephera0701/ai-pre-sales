{
  "name": "document-generator-service",
  "version": "1.0.0",
  "description": "Document generation service for PDF, Word, and Excel reports",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/ tests/"
  },
  "dependencies": {
    "aws-sdk": "^2.1490.0",
    "pdfkit": "^0.13.0",
    "docx": "^8.5.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.52.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  }
}